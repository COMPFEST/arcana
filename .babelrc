{
    "presets":[
        "@babel/preset-react",
        "@babel/preset-typescript",
        "@emotion/babel-preset-css-prop"
    ],
    "plugins":[
        "babel-plugin-macros",
        "@emotion/babel-plugin",
        [
            "@emotion/babel-plugin-jsx-pragmatic",
            {
                "export":"jsx",
                "import":"__cssprop",
                "module":"@emotion/react"
            }
        ],
        [
            "@babel/plugin-transform-react-jsx",
            {
                "pragma": "__cssprop"
            }
        ]
    ]
}